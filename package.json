{
  "name": "wxt-react-starter",
  "version": "0.0.0",
  "private": true,
  "description": "manifest.json description",
  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "format": "prettier . --write --config .prettierrc.mjs",
    "postinstall": "wxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@tensorflow/tfjs": "4.22.0",
    "@wxt-dev/storage": "^1.1.1",
    "nsfwjs": "4.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/chrome": "^0.0.310",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "wxt": "^0.19.29"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "dtrace-provider",
      "esbuild",
      "spawn-sync"
    ]
  }
}
